extends layouts/layout

block content 
    h1.head_title 나의 비디오 올리기
    form(method="POST", enctype="multipart/form-data")
        label(for="video") 비디오 파일
        input(type="file", id="video", name="video", required, accept="video/*")
        input(type="text", name="title" required, placeholder="제목을 입력해주세요" )
        textarea(name="description", placeholder="비디오의 내용을 입력해주세요")
        input(type="submit", value="업로드")
    
